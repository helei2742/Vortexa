{
   "action": {
      "default_icon": {
         "128": "images/icon-128.png",
         "16": "images/icon-16.png",
         "32": "images/icon-32.png",
         "48": "images/icon-48.png"
      },
      "default_popup": "/popup.html"
   },
   "background": {
      "service_worker": "./scripts/background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "./scripts/contentscript.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "./scripts/inpage.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start",
      "world": "MAIN"
   } ],
   "content_security_policy": {
      "extension_pages": "frame-ancestors 'none'; script-src 'self' 'wasm-unsafe-eval'; object-src 'none'"
   },
   "description": "Use HaHa for the best experience on Monad. We provide the tools that help you stay informed and degen on the go.",
   "host_permissions": [ "file://*/*", "http://*/*", "https://*/*" ],
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "HaHa Wallet",
   "permissions": [ "storage", "unlimitedStorage", "clipboardWrite", "activeTab", "notifications", "alarms" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.22",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "/home.html" ]
   } ]
}
